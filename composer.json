{
  "name": "leafwrap/laravel-installer",
  "description": "Installation for deploying or running laravel project by server requirements",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Leafwrap\\LaravelInstaller\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Ashraf Emon",
      "email": "ashraf.emon143@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "require": {},
  "extra": {
    "laravel": {
      "providers": [
        "Leafwrap\\LaravelInstaller\\LaravelInstallerServiceProvider"
      ]
    }
  }
}
